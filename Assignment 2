public class Assignment2{
    public static void main(String[] args) throws Exception {

         // for looping
         for(int i = 5; i != 0; i-- ){
             if (i==5){
                System.out.println("Jasper");
                break;

             } 
         }
        //do while
         int surnameCount = 5;

         do {
            System.out.println("Deguito");
            surnameCount--;
         } 
         while (surnameCount != 0);

        //while
         int nicknameCount = 5;

         while (nicknameCount !=1){
            nicknameCount--;

            if (nicknameCount==1){
                System.out.println("jaja");
                continue;

                
            }
         }






    }
}
